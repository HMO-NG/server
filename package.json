  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "connect-session-knex": "^4.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "mysql2": "^3.10.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.12.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/node": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.24.7",
    "cross-env": "^7.0.3",
    "debug": "4.3.5",
    "knex-cli": "^1.0.2",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.3",
    "rimraf": "^5.0.7",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0"
