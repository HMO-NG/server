{
  "name": "hmo-server",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test mocha ./test/* --exit",
    "test:windows": "cross-env NODE_ENV=test mocha ./test/* --exit",
    "build": "rimraf dist && babel src --out-dir dist",
